            $("#hydroform").show();
            $("#BackbtnEACTable").hide();
            $("#btnsbmtEACTable").hide();
        });
        $("#btnsbmtEACTable").click(function () {
            Submithydrofun();
            $("#Hydropowercontbl").hide();
            $("#hydroform").show();
            $("#BackbtnEACTable").hide();
            $("#btnsbmtEACTable").hide();
        })
        $("#hydrofrmclr").click(function () {
            $("#hydroform")[0].reset();
        })
        $("#evabtn").click(function () {
            $("#containerhydro").show();
            $("#hydroform").hide();
        });
        $("#Evaporationback").click(function () {
            $("#containerhydro").hide();
            $("#hydroform").show();
        });
        $("#Evaporationsubmit").click(function () {
            Submitevaporationbtn();
            $("#containerhydro").hide();
            $("#hydroform").show();
        });
        $("#inflowdatahydrobtn").click(function () {
            $("#hydrocont2").show();
            $("#hydroform").hide();
            InflowValuefun();
            $("#BackbtnInflowtable").show();
            $("#btnsbmtInflowtable").show();
        });
        $("#BackbtnInflowtable").click(function () {
            $("#hydrocont2").hide();
            $("#hydroform").show();
            $("#BackbtnInflowtable").hide();
            $("#btnsbmtInflowtable").hide();
        });

        $("#btnsbmtInflowtable").click(function () {
            SubmitInflowfun();
            $("#hydrocont2").hide();
            $("#hydroform").show();
            $("#BackbtnInflowtable").hide();
            $("#btnsbmtInflowtable").hide();
        });


        function InflowValuefun() {
            var Obj = {
                Startingyear: parseInt($('#hydrofrmvalue3').val()),
                startingmonth: parseInt($('#hydrofrmvalue4').val()),
                numberofmonthsforanalysis: parseInt($('#hydrofrmvalue5').val()),

            };
            $.ajax({
                type: "POST",
                url: '@Url.Action("GETInflowFunc", "HydroPower")',
                data: JSON.stringify({ 'hydro': Obj }),
                contentType: "application/json; charset=utf-8",
                dataType: "json",
                success: function (result) {
                    //console.log(result);

                    var html = '';
                    //$("#AvgPerformancetbl").DataTable().destroy(true);

                    var htmlNew = '';
                    htmlNew += '<table id="InflowTable" class="table col-lg-offset-1" style="text-align:center; width:600px;">';
                    htmlNew += '<thead class="tableheader" style="text-align: center">';
                    htmlNew += '<tr>';
                    htmlNew += '<th class="col-md-2" style="text-align:center">Month-Year</th>';
                    htmlNew += '<th class="col-md-2" style="text-align:center">Inflow</th>';



                    htmlNew += '</tr>';
                    htmlNew += '</thead>';
                    htmlNew += '<tbody class="tbody" style=" color: #000 !important;font-weight: 400 !important;text-align: center">';


                    $.each(result, function (key, item) {

