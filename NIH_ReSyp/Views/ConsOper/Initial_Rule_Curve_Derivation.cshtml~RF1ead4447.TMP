    <script src="~/Scripts/jquery-3.4.1.min.js"></script>
    <script>
    $(document).ready(function () {
        $("#eactblbtn").click(function () {
            SubmitInitialRule();
            $("#InitialRuleTable").show();
            $("#BackbtnIniTial").show();
            $("#btnsbmtInitial").show();
            $("#consoperform").hide();
        });
        $("#BackbtnIniTial").click(function () {
            $("#BackbtnIniTial").hide();
            $("#btnsbmtInitial").hide();
            $("#InitialRuleTable").hide();
            $("#consoperform").show();
        });
        $("#btnsbmtInitial").click(function () {
            SubmitIitialTable();
            $("#BackbtnIniTial").hide();
            $("#btnsbmtInitial").hide();
            $("#InitialRuleTable").hide();
            $("#consoperform").show();
        });
        $("#evaporationbtn").click(function () {
            $("#conservationEvaporation").show();
            $("#consoperform").hide();
        })
        $("#Consevaporationbackbtn").click(function () {
            $("#conservationEvaporation").hide();
            $("#consoperform").show();
        });
        $("#Consevaporationsubmitbtn").click(function () {
            Submitevaporation();
            $("#conservationEvaporation").hide();
            $("#consoperform").show();
        });

        $("#consfrmclr2").click(function () {
            $("#consoperform")[0].reset();
        });
        $("#inflowdatabtn").click(function () {
            $("#containerformonth2").show();
            $("#consoperform").hide();
        });
        $("#inflowBackbtn").click(function () {
            $("#containerformonth2").hide();
            $("#consoperform").show();
        });
        $("#demanddatabtn").click(function () {
            $("#DemandDataDiv").show();
            $("#consoperform").hide();
        })
        $("#Demanddataback").click(function () {
            $("#DemandDataDiv").hide();
            $("#consoperform").show();
        });

        function SubmitInitialRule() {
            var Obj = {
                Dead_storage_capacity: parseInt($('#numberofpointseac').val()),
            };
            $.ajax({
                type: "POST",
                url: '@Url.Action("GetInitialRule", "ConsOper")',
                data: JSON.stringify({ 'initialRuleCurve': Obj }),
                contentType: "application/json; charset=utf-8",
                dataType: "json",
                success: function (result) {
                    console.log(result);

                    var html = '';
                    //$("#AvgPerformancetbl").DataTable().destroy(true);

                    var htmlNew = '';
                    htmlNew += ' <table class="table col-lg-offset-3" style="text-align:center; width:600px" id="Initialruletable">';
                    htmlNew += '<thead class="tableheader" style="text-align: center">';
                    htmlNew += '<tr>';
                    htmlNew += ' <th class="col-md-2" style="text-align:center">Elevation</th>';
                    htmlNew += ' <th class="col-md-2" style="text-align:center">Area</th>';
                    htmlNew += ' <th class="col-md-2" style="text-align:center">Capacity</th>';


                    htmlNew += '</tr>';
                    htmlNew += '</thead>';
                    htmlNew += '<tbody class="tbody" style=" color: #000 !important;font-weight: 400 !important;text-align: center">';


                    $.each(result, function (key, item) {

                        htmlNew += '<tr>';

                        htmlNew += '<td style="font-size: 12px" contenteditable="true">' + item.Area + '</td>';
                        htmlNew += '<td style="font-size: 12px" id="values2" contenteditable="true">' + item.Elevation + '</td>';
                        htmlNew += '<td style="font-size: 12px" id="values" contenteditable="true">' + item.Capacity + '</td>';

                        htmlNew += '</tr>';
                    });
                    htmlNew += '</tbody>';
                    htmlNew += '</table>';

                    $('#InitialRuleTable').html(htmlNew);


                }
            });
        }
        function SubmitIitialTable() {
            var customers = new Array();
            $("#Initialruletable TBODY TR").each(function () {
                var row = $(this);
                   var customer = {};
                   customer.Area = (row.find("TD").eq(0).html());
                   customer.Elevation = parseInt(row.find("TD").eq(1).html());
                   customer.Capacity = parseInt(row.find("TD").eq(2).html());
                customers.push(customer);
                //console.log(customers)

                dataToPost = JSON.stringify({ newlistofeac: customers });

            jQuery.ajax({
            type: "POST",
            url: '@Url.Action("GetInitialRuleResult", "ConsOper")',
            traditional: true,
             data: dataToPost,
            contentType: "application/JSON; charset=utf-8",
                dataType: 'JSON',
            cache: false,
                success: function (r) {
                    console.log(r);
            }
            });
            });
        }
         function Submitevaporation() {
            var Obj = {
                January: parseInt($('#january').val()),
                February: parseInt($('#february').val()),
                March: parseInt($('#march').val()),
                April: parseInt($('#april').val()),
                May: parseInt($('#may').val()),
                June: parseInt($('#june').val()),
                July: parseInt($('#july').val()),
                August: parseInt($('#august').val()),
                September: parseInt($('#september').val()),
                October: parseInt($('#october').val()),
                November: parseInt($('#november').val()),
                December: parseInt($('#december').val()),
            };


            $.ajax({
                type: "POST",
                url: '@Url.Action("Evaporation", "ConsOper")',
                data: JSON.stringify({ 'evaporationDepth': Obj }),
                contentType: "application/json; charset=utf-8",
                dataType: "json",
                success: function (result) {
                   // alert(result);

                }
            });
        }
    });

    </script>



