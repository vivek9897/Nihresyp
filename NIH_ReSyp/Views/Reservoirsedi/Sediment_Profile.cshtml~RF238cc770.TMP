    </div>
</div>

    <script src="~/Scripts/jquery-3.4.1.min.js"></script>
    <script>
    $(document).ready(function () {
        $("#sedi2btn").click(function () {
            $("#sedimentProfile").show();
            NewElevationfunc2();
            $("#sediform2").hide();
            $("#sedimentProfileBack").show();
            $("#sedimentProfilesubmit").show();
        })
        $("#sedimentProfileBack").click(function () {
            $("#sedimentProfile").hide();
            $("#sediform2").show();
            $("#sedimentProfileBack").hide();
            $("#sedimentProfilesubmit").hide();
        });
        $("#sedimentProfilesubmit").click(function () {
            SubmitProfile();
            $("#sedimentProfile").hide();
            $("#sediform2").show();
            $("#sedimentProfileBack").hide();
            $("#sedimentProfilesubmit").hide();
        })
        $("#sedifrmclr2").click(function () {
            $("#sediform2")[0].reset();
        })
          function NewElevationfunc2() {
            var Obj = {
                NumbersofLevelsineac: parseInt($('#sedifrm2value3').val()),
            };
            $.ajax({
                type: "POST",
                url: '@Url.Action("SedimentProfileFun", "Reservoirsedi")',
                data: JSON.stringify({ 'reservoirsedi2': Obj }),
                contentType: "application/json; charset=utf-8",
                dataType: "json",
                success: function (result) {
                console.log(result);
                    var html = '';
                    var htmlNew = '';
                    htmlNew += ' <table class="table col-lg-offset-3" style="text-align:center; width:600px" id="tableprofile">';
                    htmlNew += '<thead class="tableheader" style="text-align: center">';
                    htmlNew += '<tr>';
                    htmlNew += ' <th class="col-md-2" style="text-align:center">Elevation</th>';
                    htmlNew += ' <th class="col-md-2" style="text-align:center">Area</th>';
                    htmlNew += ' <th class="col-md-2" style="text-align:center">Capacity</th>';
                    htmlNew += '</tr>';
                    htmlNew += '</thead>';
                    htmlNew += '<tbody class="tbody" style=" color: #000 !important;font-weight: 400 !important;text-align: center">';
                    $.each(result, function (key, item) {
                        htmlNew += '<tr>';
                        htmlNew += '<td style="font-size: 12px" contenteditable="true">' + item.Elevation + '</td>';
                        htmlNew += '<td style="font-size: 12px" id="values2" contenteditable="true">' + item.Area + '</td>';
                        htmlNew += '<td style="font-size: 12px" id="values" contenteditable="true">' + item.Capacity + '</td>';

                        htmlNew += '</tr>';
                    });
                    htmlNew += '</tbody>';
                    htmlNew += '</table>';

                    $('#sedimentProfile').html(htmlNew);

                }
            });
        }

        function SubmitProfile() {
            var customers = new Array();
            $("#tableprofile TBODY TR").each(function () {
                var row = $(this);
                var customer = {};

                customer.Elevation = parseInt(row.find("TD").eq(0).html());
                customer.Area = parseInt(row.find("TD").eq(1).html());
                customer.Capacity = parseInt(row.find("TD").eq(2).html());


                customers.push(customer);
                //console.log(customers)

                dataToPost = JSON.stringify({ sedimentprofileEacTbls: customers });

            jQuery.ajax({
            type: "POST",
            url: '@Url.Action("EacTableprofile", "Reservoirsedi")',
            traditional: true,
             data: dataToPost,
            contentType: "application/JSON; charset=utf-8",
                dataType: 'JSON',
            cache: false,
                success: function (r) {
                    console.log(r);

            }
            });
            });
        }
        });

    </script>


